{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "sourceType": "script",
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "printWidth": 120,
            "endOfLine": "auto"
        }]
    },
    "env": {
        "commonjs": true
    }
}